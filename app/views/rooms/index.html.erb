        <div id="mod_pukkaSimpleRooms" class="moduleOutput viewList">
          <!---RUBY-->
          <div class="moduleBody">
            <%
            def get_pictures(id,crop)
              @files = []
              Dir.foreach(Room.find(id).foto + crop) do |foto|
               @files.push(Room.find(id).foto[7..-1] + crop + foto) if foto.include?("JPG")
             end
             return @files

             end%>

             <%cnt = 0%>
             <% @rooms.each do |room| %>
                        <% cnt+=1 %>
             <div class="moduleItemTab acc_<%=cnt%>">
              <div class="category item<%=cnt.to_s+ ' ' + room.name + ' '%> <%=p 'selected' if cnt == 1%>">
                <h3 class="accordionTitleModule"><%=room.name%><span class="button"></span></h3>
                <div class="accordion-content">
                  <div class="module<%="23"+cnt.to_s%>Nav tabs">
                    <ul class="resp-tabs-list">
                      <li id='tab<%=cnt+1%>'><span><%=room.name%></span></li>
                      <style>
                        .module<%="23"+cnt.to_s%>Nav.tabs .resp-tabs-list li{width:calc((100% / 1) - .6em); max-width: 150px;}
                      </style>
                    </ul>
                  </div>

                  <div class="mod_content resp-tabs-container">
                    <div  class="itemPreview alt" id="item<%=cnt+1%>">
                      <div class="itemPreviewImageSection">
                        <div class="icon"></div>
                        <div class="prevImage prevImage-<%="2"+cnt.to_s%>"></div>
                        <div class="nextImage nextImage-<%="2"+cnt.to_s%>"></div>



                        <div class="roomSlideshow-<%="2"+cnt.to_s%>" data-cycle-swipe="true">
                          <% thumbs_1400_502 = get_pictures(room.id,'__thumbs_1400_502_crop/') %>
                          <% thumbs_222_119 = get_pictures(room.id,'__thumbs_222_119_crop/') %>
                          <% 0.upto(thumbs_222_119.count-1).each do |fot| %>
                          <a class='slide' href="<%=thumbs_1400_502[fot]%>" rel="slideShow-<%="2"+cnt.to_s%>">
                            <img src="<%=thumbs_222_119[fot]%>" class="slide" width="222" height="119" />
                          </a>
                          <%end%>
                          <%thumbs_1400_502 =[]%>
                          <%thumbs_222_119 = []%> 
                        </div><!-- END .roomSlideshow -->             </div><!-- END .itemPreviewImageSection -->

                        <div class='itemContent'>
                          <h3 class="itemContentTitle"><%=room.name%></h3>
                          <span class="content"><p><%=room.description%></p></span>
                          <div class="read-more toggleContent">Узнать подробности</div>
                          <div class="read-less toggleContent">Скрыть подробности</div>
                        </div>
                        <div class="itemBottomContent">
                          <a class="booking-link button_bg" href="tel:+79184007026" target="_blank"><span class="booking-link-text">Узнайте цены сейчас!</span></a>           </div><!-- END .itemBottomContent -->

                          <script type='text/javascript'>
                            jQuery(function() {
                      if(jQuery().fancybox && $("#layout").css("float")!="none"){ //namespace check
                        var fancyBoxConf = {
                          'transitionIn'  : 'fade',
                          'transitionOut' : 'fade',
                          'speedIn':500,
                          'speedOut':500
                        };
                        $('a.slide[rel="slideShow-<%="2"+cnt.to_s%>"]').fancybox(fancyBoxConf);
                      }else{
                        $('img.slide').parent('.slide').removeAttr('href');
                      }


                      jQuery("#mod_pukkaSimpleRooms .category .itemPreview").each(function(){
                        jQuery(this).find('.icon').click(function(){
                          jQuery(jQuery(this).parent().find('a.slide.cycle-slide-active[rel="slideShow-<%="2"+cnt.to_s%>"]')).trigger('click');
                          return false;
                        });
                      });
                      jQuery('.roomSlideshow-<%="2"+cnt.to_s%>').cycle({
                        fx:     'fade',
                        speed:  '2000',
                        timeout: 7000,
                        slideResize:0,
                        prev: '.prevImage-<%="2"+cnt.to_s%>',
                        next: '.nextImage-<%="2"+cnt.to_s%>',
                        slides: '> a.slide'
                      });
                    });
                  </script>
                </div><!-- END  class="itemPreview alt" -->
              </div> <!--END accordion content -->
            </div><!-- END mod_content resp-tabs-container -->
          </div><!--END "category item<%=cnt.to_s+ ' ' + room.name + ' '%> <%=p 'selected' if cnt == 1%>" -->

        </div><!--END .moduleItemTab-->
        <%end%>

        <script type="text/javascript">

          jQuery(document).ready(function() {
            $('.moduleItemTab').easyResponsiveTabs({ closed: true });

            $('.read-more').click(function(){
              contentHeight = 0;
              $(this).parent().find("span.content").children().each(function(){ contentHeight += ($(this).outerHeight(true)); });
              $(this).parent().find('span.content').addClass("open").stop().animate({ height : contentHeight },200, function(){
                $(this).parent().find('span.content').css("height","auto");
              });
              $(this).hide();
              $(this).parent().addClass("open").find('.read-less').show();
            });
            $('.read-less').click(function(){
              $(this).parent().find('span.content').stop().animate({ height : '5em' },200).removeClass("open");
              $(this).hide();
              $(this).parent().removeClass("open").find('.read-more').show();
            });

          });
        </script>

      </div>
    </div>

    
